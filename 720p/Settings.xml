		<control type="panel" id="60">
			<posx>200</posx>
			<posy>200</posy>
			<width>600</width>
			<height>600</height>
			<onleft>60</onleft>
			<onright>60</onright>
			<onup>60</onup>
			<ondown>60</ondown>
			<scrolltime>200</scrolltime>
			<viewtype label="536">icon</viewtype>
			<include>Animation_ZoomedByMenu</include>
			<include>Animation_SettingsFloat</include>
			<include>Animation_ZoomInZoomOut</include>
			<itemlayout height="200" width="200">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>200</width>
					<height>200</height>
					<texture>settings/button.png</texture>
					<colordiffuse>ButtonFilterColor</colordiffuse>
					<animation effect="zoom" start="80" end="80" center="100,100" time="0" condition="true">Conditional</animation>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>200</width>
					<height>200</height>
					<texture>$INFO[ListItem.Icon]</texture>
					<colordiffuse>white</colordiffuse>
					<animation effect="zoom" start="80" end="80" center="100,100" time="0" condition="true">Conditional</animation>
				</control>
			</itemlayout>
			<focusedlayout height="200" width="200">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>200</width>
					<height>200</height>
					<texture>settings/button.png</texture>
					<colordiffuse>ButtonFilterColor</colordiffuse>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="80" end="100" center="100,100" time="300" tween="cubic" easing="in" />
						<effect type="fade" start="0" end="100" time="300" />
					</animation>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>200</width>
					<height>200</height>
					<texture>$INFO[ListItem.Icon]</texture>
					<animation type="Focus" reversible="false">
						<effect type="zoom" start="80" end="100" center="100,100" time="300" tween="cubic" easing="in" />
						<effect type="fade" start="0" end="100" time="300" />
					</animation>
				</control>
			</focusedlayout>
			<content>
				<item id="1">
					<description>Video</description>
					<label>$LOCALIZE[3]</label>
					<icon>settings/settings_video.png</icon>
					<onclick>ActivateWindow(VideosSettings)</onclick>
