<!--Alaska-->
<window id="122">
	<defaultcontrol always="true">2</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="group">
			<include>Animation_ZoomOutZoomIn</include>
			<control type="image">
				<description>Panel</description>
				<posx>192</posx>
				<posy>94</posy>
				<width>896</width>
				<height>504</height>
				<texture border="100">common/common_panel.png</texture>
				<colordiffuse>PanelDiffuse</colordiffuse>
			</control>
			<control type="image">
				<posx>234</posx>
				<posy>184</posy>
				<width>812</width>
				<height>2</height>
				<texture>common/divider.png</texture>
				<colordiffuse>PanelDiffuseDivider</colordiffuse>
			</control>
			<control type="label">
				<description>header label</description>
				<posx>260</posx>
				<posy>138</posy>
				<width>760</width>
				<height>34</height>
				<align>center</align>
				<label>$LOCALIZE[250] - $INFO[Visualisation.Name]</label>
				<include>DialogHeader</include>
			</control>
			<control type="label" id="4">
				<description>No Settings Label</description>
				<posx>260</posx>
				<posy>260</posy>
				<width>760</width>
				<align>center</align>
				<label>$LOCALIZE[13389]</label>
				<include>DialogLabelLarge</include>
			</control>
			<control type="list" id="2">
				<posx>260</posx>
				<posy>204</posy>
				<width>760</width>
				<height>368</height>
				<onup>2</onup>
				<ondown>2</ondown>
				<itemlayout height="30" width="760">
					<control type="label">
						<posx>0</posx>
						<posy>0</posy>
						<width>760</width>
						<height>30</height>
						<aligny>center</aligny>
						<font>Font_PanelItemLabel</font>
						<textcolor>PanelListItem</textcolor>
						<selectedcolor>ListSelectedNF</selectedcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</itemlayout>
				<focusedlayout height="30" width="760">
					<control type="label">
						<posx>0</posx>
						<posy>0</posy>
						<width>760</width>
						<height>30</height>
						<aligny>center</aligny>
						<font>Font_ListTitleBig</font>
						<textcolor>TextFocused</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</focusedlayout>
			</control>
		</control>
		<include>Object_DebugGrid</include>
		<control type="group">
			<include>Debug</include>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>
	</controls>
</window>