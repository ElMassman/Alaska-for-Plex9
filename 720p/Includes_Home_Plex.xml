<includes>
	<!-- Home Plex -->
	<include name="Home_MenuPlex">
		<control type="fixedlist" id="300">
			<posx>-285</posx>
			<posy>175</posy>
			<height>400</height>
			<width>1730</width>
			<onright>300</onright>
			<onleft>300</onleft>
