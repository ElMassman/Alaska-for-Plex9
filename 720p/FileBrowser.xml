<!--Alaska-->
<window id="126">
	<defaultcontrol always="true">450</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<include>Global_Background</include>
		<include>Global_Filter</include>
		<control type="fixedlist" id="450">
			<posx>0</posx>
			<posy>94</posy>
			<height>530</height>
			<width>790</width>
			<onup>450</onup>
			<ondown>450</ondown>
			<onleft>7001</onleft>
			<onright>60</onright>
			<scrolltime>160</scrolltime>
			<pagecontrol>60</pagecontrol>
			<viewtype label="">list</viewtype>
			<orientation>vertical</orientation>
			<focusposition>6</focusposition>
			<include>Animation_ZoomInZoomOut</include>
			<animation type="Conditional" condition="ControlGroup(7001).HasFocus | Window.IsActive(111) | Window.IsActive(106)">
				<effect type="zoom" time="600" center="640,360" start="100" end="50" tween="cubic" easing="inout" />
				<effect type="fade" time="400" start="100" end="20" />
			</animation>
			<itemlayout width="870" height="40">
				<control type="label">
					<posx>770</posx>
					<posy>0</posy>
					<width>740</width>
					<height>46</height>
					<align>right</align>
					<aligny>center</aligny>
					<font>Font_ListTitleNF</font>
					<textcolor>Text</textcolor>
					<selectedcolor>ListSelectedNF</selectedcolor>
					<label>$INFO[ListItem.Label]</label>
					<scroll>false</scroll>
				</control>
			</itemlayout>
			<focusedlayout width="870" height="50">
				<control type="fadelabel">
					<posx>30</posx>
					<posy>4</posy>
					<width>740</width>
					<height>50</height>
					<align>right</align>
					<aligny>center</aligny>
					<scrollout>false</scrollout>
					<pauseatend>1800</pauseatend>
					<scrollspeed>40</scrollspeed>
					<scroll>true</scroll>
					<font>Font_ListTitleBigger</font>
					<textcolor>ListTitleFO</textcolor>
					<label>$INFO[ListItem.Label]</label>
					<animation effect="slide" end="0,-12" condition="!IsEmpty(ListItem.Size)">Conditional</animation>
				</control>
				<control type="fadelabel">
					<posx>30</posx>
					<posy>30</posy>
					<width>740</width>
					<height>30</height>
					<align>right</align>
					<aligny>center</aligny>
					<scrollout>false</scrollout>
					<pauseatend>1800</pauseatend>
					<scrollspeed>40</scrollspeed>
					<scroll>true</scroll>
					<font>Font_EpisodeDetailNF</font>
					<textcolor>ListTitleFO</textcolor>
					<label>$INFO[ListItem.Size]</label>
					<visible>!IsEmpty(ListItem.Size)</visible>
				</control>
			</focusedlayout>
		</control>
		<control type="image">
			<description>Large Icon Image</description>
			<posx>820</posx>
			<posy>122</posy>
			<width>390</width>
			<height>482</height>
			<aspectratio align="left" aligny="center">keep</aspectratio>
			<texture>$INFO[Container(450).ListItem.Icon]</texture>
			<bordertexture border="8">common/thumb_border.png</bordertexture>
			<bordersize>8</bordersize>
			<include>Animation_ZoomInZoomOut</include>
			<animation type="Conditional" condition="ControlGroup(7001).HasFocus | Window.IsActive(111) | Window.IsActive(106)">
				<effect type="zoom" time="600" center="640,360" start="100" end="50" tween="cubic" easing="inout" />
				<effect type="fade" time="400" start="100" end="20" />
			</animation>
		</control>
		<!-- view options -->
		<include>Global_ContextFilter</include>
		<control type="grouplist" id="7001">
			<width>1000</width>
			<height>40</height>
			<posx>360</posx>
			<onup>450</onup>
			<ondown>450</ondown>
			<itemgap>0</itemgap>
			<include condition="!Skin.HasSetting(TotalLockdown)">allowmenu_7001</include>
			<include condition="Skin.HasSetting(TotalLockdown)">nomenu</include>
			<include>Animation_ZoomInMenu</include>
			<orientation>horizontal</orientation>
			<posy>330</posy>
			<control type="button" id="413">
				<description>Ok</description>
				<label>$LOCALIZE[186]</label>
				<include>WindowMenuButton</include>
				<animation type="conditional" condition="!ControlGroup(7001).HasFocus(413)">
					<effect type="zoom" center="100,0" start="100" end="70" time="200" tween="cubic" easing="in" />
					<effect type="fade" start="100" end="75" time="200" />
				</animation>
			</control>
			<control type="button" id="414">
				<description>Cancel</description>
				<label>$LOCALIZE[222]</label>
				<include>WindowMenuButton</include>
				<animation type="conditional" condition="!ControlGroup(7001).HasFocus(414)">
					<effect type="zoom" center="100,0" start="100" end="70" time="200" tween="cubic" easing="in" />
					<effect type="fade" start="100" end="75" time="200" />
				</animation>
			</control>
			<control type="button" id="415">
				<description>Create Folder</description>
				<label>$LOCALIZE[119]</label>
				<include>WindowMenuButton</include>
				<animation type="conditional" condition="!ControlGroup(7001).HasFocus(415)">
					<effect type="zoom" center="100,0" start="100" end="70" time="200" tween="cubic" easing="in" />
					<effect type="fade" start="100" end="75" time="200" />
				</animation>
			</control>
		</control>
		<control type="grouplist">
			<width>1000</width>
			<height>40</height>
			<posx>360</posx>
			<onup>7050</onup>
			<ondown>7050</ondown>
			<itemgap>0</itemgap>
			<include condition="!Skin.HasSetting(TotalLockdown)">allowmenu_7001</include>
			<include condition="Skin.HasSetting(TotalLockdown)">nomenu</include>
			<include>Animation_ZoomInMenu</include>
			<orientation>horizontal</orientation>
			<posy>300</posy>
			<!-- ok -->
			<control type="button" id="7040">
				<include>WindowMenuLabel</include>
				<label>$LOCALIZE[424]</label>
				<animation type="conditional" condition="!ControlGroup(7001).HasFocus(413)">
					<effect type="zoom" center="100,30" start="100" end="70" time="200" tween="cubic" easing="in" />
					<effect type="fade" start="100" end="75" time="200" />
				</animation>
			</control>
			<!-- cancel -->
			<control type="button" id="7041">
				<include>WindowMenuLabel</include>
				<label>$LOCALIZE[424]</label>
				<animation type="conditional" condition="!ControlGroup(7001).HasFocus(414)">
					<effect type="zoom" center="100,30" start="100" end="70" time="200" tween="cubic" easing="in" />
					<effect type="fade" start="100" end="75" time="200" />
				</animation>
			</control>
			<!-- new folder -->
			<control type="button" id="7042">
				<include>WindowMenuLabel</include>
				<label>$LOCALIZE[40252]</label>
				<animation type="conditional" condition="!ControlGroup(7001).HasFocus(415)">
					<effect type="zoom" center="100,30" start="100" end="70" time="200" tween="cubic" easing="in" />
					<effect type="fade" start="100" end="75" time="200" />
				</animation>
			</control>
		</control>
		<!-- Heading -->
		<control type="group">
			<posx>30</posx>
			<posy>30</posy>
			<include>Animation_SlideDownFadeOut</include>
			<control type="label" id="411">
				<label>-</label>
				<include>Global_HeadingSectionVars</include>
			</control>
			<control type="label">
				<label>$INFO[ListItem.path]</label>
				<include>Global_HeadingDetailVars</include>
			</control>
		</control>
		<!-- Scroll Bar -->
		<control type="scrollbar" id="60">
			<include>Animation_ZoomInZoomOut</include>
			<include>Animation_ZoomedByMenu</include>
			<posx>784</posx>
			<posy>102</posy>
			<width>26</width>
			<height>526</height>
			<visible allowhiddenfocus="true">Control.IsVisible(450) + Control.HasFocus(60)</visible>
			<texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
			<textureslidernibfocus>ScrollBarV_Nib_Focus.png</textureslidernibfocus>
			<onleft>450</onleft>
			<onright>7001</onright>
			<showonepage>true</showonepage>
			<orientation>vertical</orientation>
			<animation effect="fade" start="0" end="100" time="400" condition="Control.HasFocus(60)">Conditional</animation>
		</control>
		<include>Global_Time</include>
		<include>Global_RSS</include>
		<include>Object_DebugGrid</include>
		<control type="group">
			<include>Debug</include>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>
	</controls>
</window>