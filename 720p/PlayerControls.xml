<!--Alaska-->
<window type="dialog" id="400">
	<defaultcontrol always="true">404</defaultcontrol>
	<visible>Player.HasMedia + Window.IsActive(PlayerControls) + !Window.IsActive(FullscreenVideo) + !Window.IsActive(Visualisation) + [Window.IsActive(MusicOverlay) | Window.IsActive(VideoOverlay)]</visible>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<controls>
		<control type="group" id="401">
			<posx>32</posx>
			<posy>664</posy>
			<visible allowhiddenfocus="true">Controlgroup(401).HasFocus + !Window.IsVisible(125)</visible>
			<animation type="WindowOpen">
				<effect type="fade" start="0" end="100" time="120" delay="120" />
			</animation>
			<animation type="WindowClose">
				<effect type="fade" start="100" end="0" time="80" />
			</animation>
			<control type="button" id="402">
				<description>Previous</description>
				<posx>0</posx>
				<posy>0</posy>
				<onleft>409</onleft>
				<onright>403</onright>
				<onup>411</onup>
				<ondown>410</ondown>
				<height>46</height>
				<width>46</width>
				<label>-</label>
				<texturefocus border="1">osd/button_prev_fo.png</texturefocus>
				<texturenofocus border="1">osd/button_prev_nf.png</texturenofocus>
				<onclick>PlayerControl(Previous)</onclick>
			</control>
			<control type="button" id="403">
				<description>Rewind</description>
				<posx>40</posx>
				<posy>0</posy>
				<onleft>402</onleft>
				<onright>404</onright>
				<onup>411</onup>
				<ondown>410</ondown>
				<height>46</height>
				<width>46</width>
				<label>-</label>
				<texturefocus border="1">osd/button_rew_fo.png</texturefocus>
				<texturenofocus border="1">osd/button_rew_nf.png</texturenofocus>
				<onclick>PlayerControl(rewind)</onclick>
			</control>
			<control type="togglebutton" id="404">
				<description>Play/Pause</description>
				<posx>80</posx>
				<posy>0</posy>
				<onleft>403</onleft>
				<onright>405</onright>
				<onup>411</onup>
				<ondown>410</ondown>
				<height>46</height>
				<width>46</width>
				<label>-</label>
				<texturefocus border="1">osd/button_pause_fo.png</texturefocus>
				<texturenofocus border="1">osd/button_pause_nf.png</texturenofocus>
				<usealttexture>Player.Paused | Player.Forwarding | Player.Rewinding</usealttexture>
				<alttexturefocus border="1">osd/button_play_fo.png</alttexturefocus>
				<alttexturenofocus border="1">osd/button_play_nf.png</alttexturenofocus>
				<disabledcolor>-</disabledcolor>
				<colordiffuse>-</colordiffuse>
				<onclick>PlayerControl(play)</onclick>
			</control>
			<control type="button" id="405">
				<description>Stop</description>
				<posx>120</posx>
				<posy>0</posy>
				<onleft>404</onleft>
				<onright>406</onright>
				<onup>411</onup>
				<ondown>410</ondown>
				<height>46</height>
				<width>46</width>
				<label>-</label>
				<texturefocus border="1">osd/button_stop_fo.png</texturefocus>
				<texturenofocus border="1">osd/button_stop_nf.png</texturenofocus>
				<onclick>PlayerControl(stop)</onclick>
			</control>
			<control type="button" id="406">
				<description>Forward</description>
				<posx>160</posx>
				<posy>0</posy>
				<onleft>405</onleft>
				<onright>407</onright>
				<onup>411</onup>
				<ondown>410</ondown>
				<height>46</height>
				<width>46</width>
				<label>-</label>
				<texturefocus border="1">osd/button_ff_fo.png</texturefocus>
				<texturenofocus border="1">osd/button_ff_nf.png</texturenofocus>
				<onclick>PlayerControl(forward)</onclick>
			</control>
			<control type="button" id="407">
				<description>Next</description>
				<posx>200</posx>
				<posy>0</posy>
				<onleft>406</onleft>
				<onright>408</onright>
				<onup>411</onup>
				<ondown>410</ondown>
				<height>46</height>
				<width>46</width>
				<label>-</label>
				<texturefocus border="1">osd/button_next_fo.png</texturefocus>
				<texturenofocus border="1">osd/button_next_nf.png</texturenofocus>
				<onclick>PlayerControl(Next)</onclick>
			</control>
			<control type="button" id="408">
				<description>Repeat</description>
				<posx>240</posx>
				<posy>0</posy>
				<onleft>407</onleft>
				<onright>409</onright>
				<onup>411</onup>
				<ondown>410</ondown>
				<height>46</height>
				<width>46</width>
				<label>-</label>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
				<onclick>PlayerControl(repeat)</onclick>
			</control>
			<control type="image">
				<posx>240</posx>
				<posy>0</posy>
				<width>46</width>
				<height>46</height>
				<texture border="1">osd/button_repeat_nf.png</texture>
				<visible>!Playlist.IsRepeat + !Playlist.IsRepeatOne</visible>
				<visible>!Control.HasFocus(408)</visible>
			</control>
			<control type="image">
				<posx>240</posx>
				<posy>0</posy>
				<width>46</width>
				<height>46</height>
				<texture border="1">osd/button_repeat_fo.png</texture>
				<visible>!Playlist.IsRepeat + !Playlist.IsRepeatOne</visible>
				<visible>Control.HasFocus(408)</visible>
			</control>
			<control type="image">
				<posx>240</posx>
				<posy>0</posy>
				<width>46</width>
				<height>46</height>
				<texture border="1">osd/button_repeat_one_nf.png</texture>
				<visible>Playlist.IsRepeatOne</visible>
				<visible>!Control.HasFocus(408)</visible>
			</control>
			<control type="image">
				<posx>240</posx>
				<posy>0</posy>
				<width>46</width>
				<height>46</height>
				<texture border="1">osd/button_repeat_one_fo.png</texture>
				<visible>Playlist.IsRepeatOne</visible>
				<visible>Control.HasFocus(408)</visible>
			</control>
			<control type="image">
				<posx>240</posx>
				<posy>0</posy>
				<width>46</width>
				<height>46</height>
				<texture border="1">osd/button_repeat_all_nf.png</texture>
				<visible>Playlist.IsRepeat</visible>
				<visible>!Control.HasFocus(408)</visible>
			</control>
			<control type="image">
				<posx>240</posx>
				<posy>0</posy>
				<width>46</width>
				<height>46</height>
				<texture border="1">osd/button_repeat_all_fo.png</texture>
				<visible>Playlist.IsRepeat</visible>
				<visible>Control.HasFocus(408)</visible>
			</control>
			<control type="togglebutton" id="409">
				<description>Random</description>
				<posx>280</posx>
				<posy>0</posy>
				<onleft>408</onleft>
				<onright>402</onright>
				<onup>411</onup>
				<ondown>410</ondown>
				<height>46</height>
				<width>46</width>
				<label>-</label>
				<texturefocus border="1">osd/button_random_fo.png</texturefocus>
				<texturenofocus border="1">osd/button_random_nf.png</texturenofocus>
				<usealttexture>Playlist.IsRandom</usealttexture>
				<alttexturefocus border="1">osd/button_random_on_fo.png</alttexturefocus>
				<alttexturenofocus border="1">osd/button_random_on_nf.png</alttexturenofocus>
				<disabledcolor>-</disabledcolor>
				<colordiffuse>-</colordiffuse>
				<onclick>PlayerControl(random)</onclick>
			</control>
			<control type="button" id="411">
				<description>Fullscreen Control</description>
				<posx>-2</posx>
				<posy>-186</posy>
				<width>330</width>
				<height>186</height>
				<texturefocus border="8">common/thumb_border.png</texturefocus>
				<texturenofocus>-</texturenofocus>
				<bordersize>8</bordersize>
				<aspectratio>scale</aspectratio>
				<colordiffuse>ButtonFilterColor</colordiffuse>
				<onleft>411</onleft>
				<onright>411</onright>
				<onup>410</onup>
				<ondown>402</ondown>
				<label>-</label>
				<texturefocus>-</texturefocus>
				<texturenofocus>-</texturenofocus>
				<onclick>fullscreen</onclick>
				<visible>Player.HasVideo</visible>
			</control>
		</control>
		<control type="button" id="410">
			<onfocus>Dialog.Close(PlayerControls)</onfocus>
		</control>
		<include>Object_DebugGrid</include>
		<control type="group">
			<include>Debug</include>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>
	</controls>
</window>