<!--Alaska-->
<window id="104">
	<animation type="WindowOpen" reversible="false">
		<effect type="zoom" start="80" end="100" center="1580,125" delay="200" easing="out" tween="back" time="300" />
		<effect type="fade" delay="200" start="0" end="100" time="300" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="zoom" start="100" end="80" center="1580,125" easing="in" tween="back" time="300" />
		<effect type="fade" start="100" end="0" time="300" />
	</animation>
	<controls>
		<control type="group">
			<animation effect="slide" end="0,-76" time="400" easing="inout" tween="cubic" condition="Window.IsVisible(101)">Conditional</animation>
			<animation effect="slide" end="0,-76" time="400" easing="inout" tween="cubic" condition="Window.IsVisible(138)">Conditional</animation>
			<visible>!Window.IsActive(113)</visible>
			<posx>880</posx>
			<posy>90</posy>
			<width>707</width>
			<height>183</height>
			<control type="group">
				<visible>!player.passthrough</visible>
				<control type="image">
					<posx>180</posx>
					<posy>-2</posy>
					<width>300</width>
					<height>74</height>
					<texture border="37,0,0,0">dialogs/back.png</texture>
					<colordiffuse>DialogBackground</colordiffuse>
					<visible>Window.IsActive(2005) | Window.IsActive(2006)</visible>
				</control>
				<control type="image">
					<posx>324</posx>
					<posy>12</posy>
					<width>46</width>
					<height>46</height>
					<texture>dialogs/dialog_back.png</texture>
					<colordiffuse>DialogBack</colordiffuse>
				</control>
				<control type="progress" id="404">
					<description>Progressbar</description>
					<posx>331</posx>
					<posy>19</posy>
					<width>32</width>
					<height>33</height>
					<texturebg>dialogs/clear.png</texturebg>
					<lefttexture>-</lefttexture>
					<midtexture>dialogs/grey.png</midtexture>
					<righttexture>-</righttexture>
					<overlaytexture>-</overlaytexture>
					<info>player.volume</info>
					<animation type="WindowOpen" reversible="false">
						<effect type="fade" delay="400" start="0" end="100" time="100" />
					</animation>
					<animation type="WindowClose" reversible="false">
						<effect type="fade" start="100" end="0" time="10" />
					</animation>
				</control>
				<control type="image">
					<posx>322</posx>
					<posy>10</posy>
					<width>50</width>
					<height>50</height>
					<texture>dialogs/volume.png</texture>
					<colordiffuse>ProgressFilterColor</colordiffuse>
				</control>
				<control type="label">
					<description>Heading Label</description>
					<posx>310</posx>
					<posy>15</posy>
					<width>627</width>
					<height>51</height>
					<textcolor>Time</textcolor>
					<align>right</align>
					<font>Font_ProgressDialog</font>
					<label>$LOCALIZE[13376]</label>
				</control>
				<control type="label">
					<posx>310</posx>
					<posy>35</posy>
					<width>627</width>
					<height>51</height>
					<textcolor>Date</textcolor>
					<align>right</align>
					<font>Font_Date</font>
					<label>$INFO[player.volume]</label>
				</control>
			</control>
			<control type="group">
				<visible>player.passthrough</visible>
				<control type="image">
					<posx>0</posx>
					<posy>-2</posy>
					<width>480</width>
					<height>74</height>
					<texture border="37,0,0,0">dialogs/back.png</texture>
					<colordiffuse>DialogBackground</colordiffuse>
					<visible>Window.IsActive(2005) | Window.IsActive(2006)</visible>
				</control>
				<control type="image">
					<posx>324</posx>
					<posy>12</posy>
					<width>46</width>
					<height>46</height>
					<texture>dialogs/dialog_back.png</texture>
					<colordiffuse>DialogBack</colordiffuse>
				</control>
				<control type="image">
					<posx>322</posx>
					<posy>10</posy>
					<width>50</width>
					<height>50</height>
					<texture>dialogs/volume.png</texture>
					<colordiffuse>ProgressFilterColor</colordiffuse>
				</control>
				<control type="label">
					<description>Heading Label</description>
					<posx>310</posx>
					<posy>15</posy>
					<width>627</width>
					<height>51</height>
					<textcolor>Time</textcolor>
					<align>right</align>
					<font>Font_ProgressDialog</font>
					<label>$LOCALIZE[13376]</label>
				</control>
				<control type="label">
					<posx>310</posx>
					<posy>35</posy>
					<width>627</width>
					<height>51</height>
					<textcolor>Date</textcolor>
					<align>right</align>
					<font>Font_Date</font>
					<label>$LOCALIZE[29802]</label>
				</control>
			</control>
		</control>
		<include>Object_DebugGrid</include>
		<control type="group">
			<include>Debug</include>
			<visible>Skin.HasSetting(debugmode)</visible>
		</control>
	</controls>
</window>